{
  "name": "realworldapp",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "cypress:smoke-firefox": "npx cypress run --spec 'cypress/e2e/workingWithApis.spec.js' --browser firefox",
    "cypress:smoke-chrome": "npx cypress run --spec 'cypress/e2e/api_headlessAuth.spec.js' --browser chrome",
    "cypress:smoke-all": "npm run cypress:smoke-firefox && npm run cypress:smoke-chrome",
    "cypress:smoke-all-parallel": "npm-run-all cypress:smoke-chrome cypress:smoke-firefox",
    "delete:reports": "rm -r cypress/reports/* || true",
    "prereport": "npm run delete:reports",
    "junit:merge": "npx junit-merge -d cypress/reports/junit -o cypress/reports/junit/results.xml",
    "cy:run:all": "npm run prereport && npx cypress run || true && npm run junit:merge"
  },
  "devDependencies": {
    "cypress": "^15.6.0",
    "cypress-mochawesome-reporter": "^4.0.2",
    "cypress-multi-reporters": "^2.0.5",
    "mocha-junit-reporter": "^2.2.1"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "npm-run-all": "^4.1.5"
  }
}
